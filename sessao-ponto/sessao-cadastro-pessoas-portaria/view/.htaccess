#RewriteEngine on
#RewriteBase /
#Options +FollowSymLinks

#RewriteCond %{HTTP_HOST} !=""
#RewriteCond %{HTTP_HOST} !^www\. [NC]
#RewriteCond %{HTTPS}s ^on(s)|
#RewriteRule ^ http%1://www.%{HTTP_HOST}%{REQUEST_URI} [R=301,L]

#RewriteRule ^([^\.]+)$ http%1://%{HTTP_HOST} [NC,L]

#RewriteRule ^footer.php$ http%1://www.stm-sistema.com.br/sessao-cadastro-pessoas-portaria/view/home.php [R=301,L]
