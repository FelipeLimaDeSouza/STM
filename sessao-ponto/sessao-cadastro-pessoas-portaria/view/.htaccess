#RewriteRule ^validarSession.php$ http%1://www.stm-sistema.com.br/sessao-cadastro-pessoas-portaria/view/home.php [R=301,L]
#RewriteRule ^modais.php$ http%1://www.stm-sistema.com.br/sessao-cadastro-pessoas-portaria/view/home.php [R=301,L]

#Options -Indexes